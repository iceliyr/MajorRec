.page {
    /*居中*/
    width: 100%;
    margin-top: 10rpx;
    float: left;
    background-color: white;
    height: auto;
    border-radius: 0;
    display: flex;
    align-items: center;
    border-right: 1rpx solid #efefef;
    border-top: 1rpx solid #efefef;
    border-bottom: 1rpx solid #efefef;
}

.right-page {
    margin-top: 50rpx;
